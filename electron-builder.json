{
  "appId": "com.electron.app",
  "directories": {
    "output": "dist"
  },
  "nsis": {
    "oneClick": false,
    "perMachine": false,
    "allowToChangeInstallationDirectory": true,
    "shortcutName": "CBSElectronToDoList"
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": ["ia32"]
      }
    ]
  },
  "linux": {
    "target": ["AppImage"]
  },
  "files": ["out/**/*"]
}
